Cell["Conversions", "Subsection"],

Cell[TextData[{
 "Often one is presented with quantities expressed in British, engineering, \
or other units.  The command ",
 StyleBox["SI[", "InlineInput",
  FontWeight->"Bold"],
 StyleBox["quantity", "InlineInput"],
 StyleBox["]", "InlineInput",
  FontWeight->"Bold"],
 " converts to SI units."
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SI", "[", "Horsepower", "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"745.7`", " ", "Watt"}]], "Output"]
}, Closed]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SI", "[", "Slug", "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"14.5939`", " ", "Kilogram"}]], "Output"]
}, Closed]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SI", "[", "BTU", "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"1055.0600000000002`", " ", "Joule"}]], "Output"]
}, Closed]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"?", "Convert"}]], "Input"],

Cell[BoxData[
 RowBox[{"\<\"Convert[old, new] converts old to a form involving the \
combination of units new.\"\>", " ", 
  ButtonBox[
   StyleBox["More\[Ellipsis]", "SR"],
   Active->True,
   BaseStyle->"AddOnsLink",
   ButtonData:>"Miscellaneous`Units`"]}]], "Print",
 CellTags->"Info3351098527-8632358"]
}, Closed]],

Cell[TextData[{
 "More generally, ",
 StyleBox["Convert[", "InlineInput",
  FontWeight->"Bold"],
 StyleBox["quantity,units", "InlineInput"],
 StyleBox["]", "InlineInput",
  FontWeight->"Bold"],
 " converts a ",
 StyleBox["quantity", "InlineInput"],
 " to a new set of dimensionally consistent ",
 StyleBox["units", "InlineInput"],
 ". "
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Convert", "[", 
  RowBox[{
   RowBox[{"0.5", " ", "Acre"}], ",", " ", 
   SuperscriptBox["Meter", "2"]}], "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"2023.4282111999999`", " ", 
  SuperscriptBox["Meter", "2"]}]], "Output"]
}, Closed]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Convert", "[", 
  RowBox[{"Parsec", ",", "LightYear"}], "]"}]], "Input"],

Cell[BoxData[
